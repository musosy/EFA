{
  "name": "efa",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start:dev": "ts-node-dev -r dotenv/config --no-notify --respawn --exit-child -r tsconfig-paths/register --transpile-only src/server",
    "start:prod": "npm run build && node dist/server.js",
    "lint": "lint-staged",
    "test": "npx jest --runInBand --color --forceExit",
    "test:cov": "npx jest --runInBand --coverage --color --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/types": "^29.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "./node_modules/.bin/eslint --max-warnings=0 --fix",
      "pretty-quick"
    ]
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "coverage"
  }
}
